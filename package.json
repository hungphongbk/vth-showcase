{
  "name": "vth-showcase",
  "version": "0.1.1-0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 4000",
    "dev:ssl": "local-ssl-proxy --source 4001 --target 4000 --cert ~/localhost.pem --key ~/localhost-key.pem",
    "prebuild:portal": "rimraf ./public/sdk",
    "build:portal": "NODE_ENV=production webpack -c ./sdk/webpack.config.js",
    "build:next": "next build",
    "build": "concurrently \"npm:build:next\" \"npm:build:portal\"",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start -p 4000",
    "start:portal": "webpack-dev-server -c ./sdk/webpack.config.js",
    "lint": "next lint",
    "graphql-codegen:watch": "NODE_ENV=development graphql-codegen --watch -r dotenv-flow/config",
    "graphql-codegen": "NODE_ENV=development graphql-codegen -r dotenv-flow/config",
    "prepare": "husky install",
    "update-sdk": "npm i @hungphongbk/vth-sdk@latest"
  },
  "dependencies": {
    "@apollo/client": "3.6.9",
    "@emotion/react": "11.10.4",
